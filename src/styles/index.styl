@import "variables";

.account {
  height: 70px;
  background: brand-purple;
  color: brand-purple;
  font-size: 14px;
  font-weight: 500;
  display: flex;
  align-items: center;
  padding: 0 10px;
  border-top: 1px solid darken(brand-purple, 30%);
  flex-shrink: 0;

  .avatar-small {
    flex-shrink: 0;
    margin: 0;
    cursor: pointer;
    transition: opacity 0.2s ease;

    &:hover {
      opacity: 0.8;
    }
  }

  .status {
    border: 2px solid brand-purple;
  }

  .account-details {
    padding: 0 10px;
    flex-grow: 1;
    user-select: text;

    .username {
      display: block;
      max-width: 72px;
    }

    .discriminator {
      display: block;
      padding-top: 1px;
      opacity: .4;
      font-size: 10px;
    }
  }

  .btn {
    width: 32px;
    height: @width;
    background: brand-purple;
    position: relative;

    &:active {
      background: #34373c;
      box-shadow: inset 0 0 4px rgba(0,0,0,.06);
    }

    &:after {
      content: ' ';
      background-repeat: no-repeat;
      background-position: center center;
      background-size: 16px 16px;
      margin-right: 5px;
      absolute: top left;
      height: 100%;
      width: 100%;
    }
  }

  .btn-mute {
    border-radius: 3px 0 0 3px;

    &:after {
      background-color: brand-purple;
    }

    &:hover:after {
      background-color: brand-purple;
    }

    &.disabled:after {
      background-color: brand-purple;
    }

    &.disabled:hover:after {
      background-color: brand-purple;
    }
  }

  .btn-deafen {
    &:after {
      background-color: brand-purple;
    }

    &:hover:after {
      background-color: brand-purple;
    }

    &.disabled:after {
      background-color: brand-purple;
    }

    &.disabled:hover:after {
      background-color: brand-purple;
    }
  }

  .btn-settings {
    border-radius: 0 3px 3px 0;
    &:after {
      background-color: brand-purple;
      opacity: .2;
    }

    &:hover:after {
      opacity: .7;
    }
  }

  .btn-mute, .btn-deafen, .btn-settings {
    transition: background-image 0.5s;
  }

  .btn-mute, .btn-deafen {
    border-right: 1px solid darken(brand-purple, 30%);
  }

  .btn-deafen, .btn-settings {
    box-shadow: inset 1px 0 0 lighten(brand-purple, 8%);
  }

  .btn-group {
    border: 1px solid darken(brand-purple, 30%);
    border-radius: 4px;
    overflow: hidden;
    flex-shrink: 0;
  }
}

